#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <rep:policy/>
    <jcr:content
        cq:allowedTemplates="[/conf/${clientName}-core/settings/wcm/templates/.*]"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:designPath="/apps/settings/wcm/design/${clientName}-core"
        cq:template="/conf/${clientName}-core/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="${clientName}-core Showcase"
        sling:resourceType="${clientName}-core/components/template/base"
        redirectTarget="/content/${clientName}-core-showcase/en">
        <image
            jcr:primaryType="nt:unstructured"
            imageRotate="0"/>
    </jcr:content>
    <en/>
</jcr:root>
