#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${clientName}-core:content-type/page]"
        cq:template="/conf/${clientName}-core/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Parallax Image"
        sling:resourceType="${clientName}-core/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${clientName}-core/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${clientName}-core/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${clientName}-core/components/details/page-details"
                    componentId="page-details_IKOBELTSF"/>
                <contentblockmenu1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${clientName}-core/components/layout/contentblockmenu"
                    componentId="contentblockmenu1"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${clientName}-core/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Parallax Image"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${clientName}-core/components/layout/container">
                        <contentblock
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${clientName}-core/components/layout/contentblock"
                            componentId="contentblock1a"
                            componentModules="[${clientName}-core:component-style-module/layout/contentblock/parallaximage]"
                            title="Parallax Image Container"
                            variant="parsys">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${clientName}-core/components/layout/container">
                                <image1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${clientName}-core/components/media/image"
                                    componentId="image1"
                                    componentTheme="[${clientName}-core:component-style-theme/media/image/headerlogo]"
                                    fileReference="/content/dam/${clientName}-core-showcase/en/multimedia/image/logo-${clientName}-core.png"/>
                                <image2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${clientName}-core/components/media/image"
                                    componentId="image2"
                                    componentTheme="[${clientName}-core:component-style-theme/media/image/headerlogo]"
                                    fileReference="/content/dam/${clientName}-core-showcase/en/multimedia/image/logo-${clientName}-core.png"/>
                            </par>
                        </contentblock>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${clientName}-core/components/content/text"
                            componentId="text_FVEVQCURR"
                            text="&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;&lt;p>Page Content&lt;/p>&${symbol_pound}xd;&${symbol_pound}xa;"
